{
  "name": "metabytes-sro/laravel-formatter",
  "description": "A formatting library that converts data output between XML, CSV, JSON, TXT, YAML and a few others.",
  "homepage": "https://github.com/metabytes-sro/laravel-formatter",
  "version": "3.5.0",
  "readme": "README.md",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Guilherme Almeida Girardi",
      "email": "guilhermeagirardi@gmail.com",
      "homepage": "https://guilhermegirardi.com",
      "role": "Developer"
    },
    {
      "name": "Quosimadu",
      "email": "Quosimadu@metabytes.eu",
      "homepage": "",
      "role": "Developer"
    }
  ],
  "keywords": [
    "php",
    "laravel",
    "format",
    "formatter",
    "output",
    "response",
    "data",
    "convert",
    "json",
    "csv",
    "xml",
    "yaml"
  ],
  "require": {
    "php": "^8.2",
    "league/csv": "^9.9",
    "mustangostang/spyc": "~0.6",
    "illuminate/support": "*"
  },
  "autoload": {
    "psr-4": {
      "MetabytesSRO\\Formatter\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "aliases": {
        "Formatter": "MetabytesSRO\\Formatter"
      }
    }
  },
  "minimum-stability": "stable",
  "scripts": {
    "test": "vendor\\bin\\phpunit"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10|^10.0|^11.0|"
  }
}
